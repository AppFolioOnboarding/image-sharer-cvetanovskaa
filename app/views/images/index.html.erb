<div class="main pt-4">
  <div class="container">
    <h1>Image Sharer</h1>
    <h3> Welcome to the Image Sharer App!</h3>
  </div>
  <div class="container pt-4">
    <div class="pull-right">
      <%= link_to "Submit an image URL", new_image_path, class: "btn btn-dark" %>
    </div>
  </div>
  <div class="container">
    <% @images.each do |img| %>
      <div>
        <%= image_tag(img.url, :class => 'img') %>
      </div>
    <% end %>
  </div>
</div>
